<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.keystorepablo.pantallas.RecursoController">
   <Button layoutX="408.0" layoutY="100.0" mnemonicParsing="false" onAction="#crearRecurso" prefHeight="25.0" prefWidth="103.0" text="crear recurso" />
    <Button layoutX="26.0" layoutY="62.0" mnemonicParsing="false" onAction="#compartirRecurso" prefHeight="25.0" prefWidth="116.0" text="compartir recurso" />
    <Button layoutX="26.0" layoutY="106.0" mnemonicParsing="false" onAction="#verificarFirma" prefHeight="25.0" prefWidth="103.0" text="verificar firma" />
    <Button layoutX="26.0" layoutY="278.0" mnemonicParsing="false" onAction="#cambiarContrasenya" prefHeight="25.0" prefWidth="103.0" text="cambiar contraseña" />
    <TableView fx:id="recursosTable" layoutX="201.0" layoutY="24.0" prefHeight="339.0" prefWidth="146.0">
        <columns>
            <TableColumn fx:id="id" prefWidth="43.0" text="Id" />
            <TableColumn fx:id="firma" prefWidth="102.0" text="Order's date" />
        </columns>
    </TableView>
    <TextField fx:id="nombreRecurso" layoutX="408.0" layoutY="24.0" prefHeight="25.0" prefWidth="95.0" promptText="Nombre Recurso" />
    <PasswordField fx:id="contrasenyaRec" layoutX="408.0" layoutY="62.0" prefHeight="25.0" prefWidth="95.0" promptText="Contraseña" />
    <PasswordField fx:id="newPassword" layoutX="30.0" layoutY="238.0" prefHeight="25.0" prefWidth="95.0" promptText="Nueva Contraseña" />
    <ComboBox fx:id="comboBox" layoutX="26.0" layoutY="24.0" prefWidth="150.0" promptText="Filter" />


</AnchorPane>
